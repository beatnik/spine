insert into content (body,breaks,icomment,keywords,logging,macros,modified,name,owner,permissions,style,title,type,usergroup) values ('<table width=\"100%\" border=\"0\" bgcolor=\"#ffffff\" cellspacing=\"1\" cellpadding=\"0\">\r\n<tr width=\"100%\"><td width=\"100%\">\r\n$error\r\n<table width=\"100%\" cellspacing=\"1\" cellpadding=\"0\" border=\"0\" bgcolor=\"#ffffff\">\r\n\r\n<tr bgcolor=\"#ffffff\">\r\n\r\n<td width=\"20%\"><b>Full Name</b></td><td width=\"20%\"><b>Login</b></td>  \r\n<td width=\"1%\">&nbsp;</td>\r\n<td width=\"1%\">&nbsp;</td>\r\n<td width=\"58%\">&nbsp;</td>\r\n</tr>\r\n\r\n<tr bgcolor=\"#ffffff\">\r\n<form method=\"post\" action=\"<?SPINE_Location?>admin/user/new/\">\r\n<td><input type=\"text\" name=\"fullname\" class=\"input\" value=\"Full Name\" size=\"30\"></td>\r\n<td><input type=\"text\" name=\"login\" value=\"Login\" class=\"input\" size=\"30\"></td>\r\n<td><input type=\"submit\" value=\"create\" class=\"button\" name=\"action\"></td></form>\r\n<td>&nbsp;</td>\r\n<td>&nbsp;</td>\r\n</tr>\r\n\r\n$userdata\r\n\r\n</table>\r\n\r\n</td></tr></table>','0','This is the template for user administration','','1','','2007-02-23 01:08:44','.admin-user','admin','0000','.admin','User Administration','','admin');
