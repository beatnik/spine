insert into content (body,breaks,icomment,keywords,logging,macros,modified,name,owner,permissions,style,title,type,usergroup) values ('      <table width=\"100%\" border=\"0\" bgcolor=\"#ffffff\" cellpadding=\"0\" cellspacing=\"0\">\r\n<tr width=\"100%\"><td width=\"100%\">\r\nStatistics for <b>$name</b>\r\n<table width=\"100%\" cellspacing=\"1\" cellpadding=\"0\" border=\"0\" bgcolor=\"#ffffff\">\r\n<tr bgcolor=\"#ffffff\">\r\n<td width=\"45%\"><b>Referer</b></td>\r\n<td><b>Requests</b> </td>  \r\n</tr>\r\n$refererlist\r\n</table>\r\n\r\n<table width=\"100%\" cellspacing=\"1\" cellpadding=\"0\" border=\"0\" bgcolor=\"#ffffff\">\r\n<tr bgcolor=\"#ffffff\">\r\n<td width=\"45%\"><b>User Agent</b></td>\r\n<td><b>Requests</b></td>  \r\n</tr>\r\n$useragentlist\r\n</table>\r\n\r\n<table width=\"100%\" cellspacing=\"1\" cellpadding=\"0\" border=\"0\" bgcolor=\"#ffffff\">\r\n<tr bgcolor=\"#ffffff\">\r\n<td width=\"45%\"><b>IP</b></td>\r\n<td><b>Requests</b></td>  \r\n</tr>\r\n$remoteaddrlist\r\n</table>\r\n\r\n<table width=\"100%\" cellspacing=\"1\" cellpadding=\"0\" border=\"0\" bgcolor=\"#ffffff\">\r\n<tr bgcolor=\"#ffffff\">\r\n<td width=\"45%\"><b>Query</b></td>\r\n<td><b>Requests</b></td>  \r\n</tr>\r\n$querylist\r\n</table>\r\n\r\n</td></tr></table>\r\n','0','This is the template for statistics administration','','1','','2006-12-31 23:00:00','.admin-stats','admin','0000','.admin','Statistics Administration','','');
insert into content (body,breaks,icomment,keywords,logging,macros,modified,name,owner,permissions,style,title,type,usergroup) values ('      <table width=\"100%\" border=\"0\" bgcolor=\"#ffffff\" cellpadding=\"0\" cellspacing=\"0\">\r\n<tr width=\"100%\">\r\n<font face=\"Verdana, Arial, Helvetica, sans-serif\" size=\"2\"><td>\r\n<form method=\"post\" action=\"<?SPINE_Location?>admin/$type/view/\"><b>View $label</b><br>\r\n<input type=\"submit\" value=\"View\" class=\"button\">&nbsp;<select name=\"name\" class=\"general\"><option value=\"\">Select one\r\n$list\r\n</select></td>\r\n</form></tr>\r\n<tr width=\"100%\"><td>\r\n<center><hr width=\"95%\"></center></td></tr>\r\n<tr width=\"100%\">\r\n<!-- Remove -->\r\n<form method=\"POST\" name=\"theform\" action=\"<?SPINE_Location?>admin/$type/remove/\" onSubmit=\"return confirm(\'You are about to delete \'+(document.theform.$type.options[document.theform.$type.selectedIndex].value)+\'! Are you sure?\');\"> <td>\r\n<b>Remove $label</b><br>\r\n<input type=\"submit\" value=\"Remove\" class=\"button\">&nbsp;<select name=\"name\" class=\"general\"><option value=\"\">Select one\r\n$list\r\n</select></td>\r\n</form></tr>\r\n<tr width=\"100%\"><td>\r\n<center><hr width=\"95%\"></center>\r\n</td></tr>\r\n<tr width=\"100%\"><td>\r\n<table width=\"100%\" cellspacing=\"1\" cellpadding=\"0\" border=\"0\" bgcolor=\"#ffffff\">\r\n<tr bgcolor=\"#ffffff\">\r\n<td width=\"5%\"><b>Page</b></td>\r\n<td><b>Requests</b></td>  \r\n</tr>\r\n$namelist\r\n</table>\r\n</td></tr></table>','0','This bit is the general statistics administration template','','1','','2006-12-31 23:00:00','.admin-stats-general','admin','0000','.admin','Statistics Administration','','admin');
insert into content (body,breaks,icomment,keywords,logging,macros,modified,name,owner,permissions,style,title,type,usergroup) values ('<script language=\"javascript\">\r\nvar http_request = false;\r\nfunction makeRequest(meth, url, parameters) \r\n{ http_request = false;\r\n  if (window.XMLHttpRequest) // Mozilla, Safari,...\r\n   { http_request = new XMLHttpRequest();\r\n     if (http_request.overrideMimeType) \r\n     { // set type accordingly to anticipated content type\r\n       //http_request.overrideMimeType(\'text/xml\');\r\n       http_request.overrideMimeType(\'text/html\');\r\n     }\r\n   } else \r\n   if (window.ActiveXObject) // IE\r\n   { try \r\n     { http_request = new ActiveXObject(\"Msxml2.XMLHTTP\");\r\n     } catch (e) \r\n     { try \r\n       { http_request = new ActiveXObject(\"Microsoft.XMLHTTP\");\r\n       } catch (e) \r\n         {}\r\n     }\r\n   }\r\n   if (!http_request) \r\n   { alert(\'Cannot create XMLHTTP instance\');\r\n     return false;\r\n   }\r\n   if (meth == \"POST\")\r\n   { http_request.onreadystatechange = alertContents;\r\n     http_request.open(\'POST\', url, false);\r\n     http_request.setRequestHeader(\"Content-type\", \"application/x-www-form-urlencoded\");\r\n     http_request.setRequestHeader(\"Content-length\", parameters.length);\r\n     http_request.setRequestHeader(\"Connection\", \"close\");\r\n     http_request.send(parameters);\r\n     return http_request.status;\r\n   }\r\n   if (meth == \"GET\")\r\n   { http_request.onreadystatechange = alertContents;\r\n     http_request.open(\'GET\', url, false);\r\n     http_request.send(null);\r\n  }\r\n}\r\n\r\nfunction alertContents() {\r\n    if (http_request.readyState == 4) \r\n    { document.getElementById(\'attrlist\').innerHTML = http_request.responseText;\r\n    } \r\n}\r\n\r\nwindow.onload=function() { makeRequest(\'POST\',\'<?SPINE_Location?>admin/attr/\', \'name=\' + encodeURI( \'$filename\' )); }\r\n</script>\r\n<div class=\"dialog\" id=\"attributes\" style=\"overflow: auto; height: 200px\">\r\n<img border=\"0\" align=\"right\" valign=\"top\" src=\"/images/delete.png\" onclick=\"document.getElementById(\'attributes\').style.display=\'none\';\"><b>&nbsp;Custom Attributes</b><br>\r\n<div style=\"width: 590px; height: 1.75em\" id=\"attrlist\">&nbsp;</div>\r\n</div>\r\n\r\n<div class=\"dialog\" id=\"tags\" style=\"width:900px; height:140px\">\r\n<img border=\"0\" align=\"right\" valign=\"top\" src=\"/images/delete.png\" onclick=\"document.getElementById(\'tags\').style.display=\'none\'; return false;\"><u>Usable tags:</u><br>&lt;?SPINE_Content?&gt;, &lt;?SPINE_Env?&gt;, &lt;?SPINE_Images?&gt;, &lt;?SPINE_Location?&gt;, &lt;?SPINE_Login?&gt;, &lt;?SPINE_Macro(\"<i>key</i>\")?&gt;, \r\n&lt;?SPINE_Message(\"<i>messagegroup</i>\"[,\"<i>id</i>\"])?&gt;, &lt;?SPINE_Message_Body?&gt;, &lt;?SPINE_Message_Comments?&gt;, &lt;?SPINE_Message_Date?&gt;, \r\n&lt;?SPINE_Message_DateTime?&gt;, &lt;?SPINE_Message_Group?&gt;, &lt;?SPINE_Message_Id?&gt;, &lt;?SPINE_Message_Owner?&gt;, &lt;?SPINE_Message_Parent_Id?&gt;,\r\n&lt;?SPINE_Message_Parent_Subject?&gt;, &lt;?SPINE_Message_Subject?&gt;, &lt;?SPINE_Message_Time?&gt;, &lt;?SPINE_Navbar(\"<i>navbarname</i>\")?&gt;,\r\n&lt;?SPINE_Page?&gt; &lt;?SPINE_Referer?&gt;, &lt;?SPINE_Search?&gt;, &lt;?SPINE_Servername?&gt;, &lt;?SPINE_Stats(\"hits\")?&gt;, &lt;?SPINE_Stats(\"modified\")?&gt;,\r\n&lt;?SPINE_Title?&gt;, &lt;?SPINE_User?&gt;, &lt;?SPINE_Webmaster?&gt;, &lt;?SPINE_XMLChanges?&gt;</div>\r\n<table width=\"1000\" border=\"0\" bgcolor=\"#ffffff\" cellpadding=\"0\" cellspacing=\"0\">\r\n\r\n<tr width=\"1000\">\r\n<form method=\"post\" action=\"<?SPINE_Location?>admin/content/save/\" name=\"editcontent\">\r\n\r\n<td colspan=\"5\" width=\"1000\">\r\n$error<b>Content Edit : $filename $lock</b><br>\r\n<input type=\"submit\" value=\"Save\" class=\"button\">\r\n<input type=\"button\" value=\"Preview\" onclick=\"window.open(\'<?SPINE_Location?>$filename\',\'Hint\',\'width=800,height=600,scrollbars=yes\'); return false;\" class=\"button\">\r\n<input type=\"button\" value=\"Advanced\" onclick=\"o=document.getElementById(\'advanced\'); if(o.style.display==\'inline\'){o.style.display=\'none\';}else{o.style.display=\'inline\';}\" class=\"button\">\r\n<input type=\"hidden\" name=\"name\" value=\"$filename\">\r\n</td></tr>\r\n\r\n<tr width=\"1000\">\r\n<td width=\"15%\">Size <sup>[<a onClick=\"return false;\" class=\"info\">?<span>This is the size of the content body.</span></a>]</sup> :</td><td width=\"25%\"> $size bytes</td>\r\n\r\n<td width=\"15%\">Last Modified <sup>[<a onClick=\"return false;\" class=\"info\">?<span>This is the last modifcation date of the content.</span></a>]</sup> : </td><td width=\"25%\">$lastmod</td>\r\n<td width=\"20%\">&nbsp;</td></tr>\r\n\r\n<tr width=\"1000\">\r\n<td width=\"15%\">\r\nTitle <sup>[<a class=\"info\" onClick=\"return false;\">?<span>The Title contains the string that can be used in conjunction with &lt;?SPINE_Title?&gt;.</span></a>]</sup> : </td><td width=\"25%\"><input type=\"text\" class=\"input\" name=\"title\" value=\"$title\" size=\"25\"></td>\r\n\r\n<td width=\"15%\">\r\nStyle <sup>[<a class=\"info\" onClick=\"return false;\">?<span>This is allows you to define a style.</span></a>]</sup> : </td><td width=\"25%\"><select name=\"style\" class=\"general\">\r\n<option>Select Style $stylelist</select></td>\r\n<td width=\"20%\">&nbsp;</td>\r\n</tr>\r\n\r\n<tr width=\"1000\">\r\n<td colspan=\"5\"  width=\"1000\">\r\n<table width=\"1000\" border=\"0\" bgcolor=\"#ffffff\" cellpadding=\"0\" cellspacing=\"0\" id=\"advanced\" style=\"display:none\">\r\n<tr width=\"1000\"><td width=\"1000\" colspan=\"5\"><img src=\"\" width=\"997\" height=\"1\"></td></tr>\r\n<tr width=\"1000\">\r\n<td width=\"20%\">Enter to &lt;br&gt; <sup>[<a class=\"info\" onClick=\"return false;\">?<span>This feature toggles converting newlines in the content to HTML breaks.</span></a>]</sup> : </td><td width=\"25%\">$breaks</td>\r\n<td width=\"20%\">\r\nLog Access <sup>[<a class=\"info\" onClick=\"return false;\">?<span>This feature toggles logging access to this Content.</span></a>]</sup> : </td><td width=\"25%\">$logging</td><td width=\"10%\">&nbsp;</td></tr>\r\n\r\n<tr bgcolor=\"#ffffff\" width=\"1000\"><td width=\"20%\">Macros <sup>[<a class=\"info\" onClick=\"return false;\">?<span>This is allows you to define Macros.</span></a>]</sup> :\r\n\r\n</td><td width=\"25%\"><select name=\"macros\" class=\"general\">\r\n<option value=\"\">none\r\n$macrolist\r\n</select></td>\r\n<td width=\"20%\">\r\nKeywords <sup>[<a class=\"info\" onClick=\"return false;\">?<span>This is allows you to define keywords for the search engine feature. Separate the words with spaces.</span></a>]</sup> : </td><td width=\"25%\">\r\n<input type=\"text\" name=\"keywords\" value=\"$keywords\" size=\"30\" class=\"input\"></td><td width=\"10%\">&nbsp;</td></tr>\r\n\r\n<tr bgcolor=\"#ffffff\" width=\"1000\">\r\n<td width=\"15%\">\r\nOwner <sup>[<a class=\"info\" onClick=\"return false;\">?<span>This shows the content owner.</span></a>]</sup> :</td><td width=\"25%\">$owner</td>\r\n\r\n<td width=\"15%\">Group <sup>[<a class=\"info\" onClick=\"return false;\">?<span>This shows the content ownergroup.</span></a>]</sup> : </td><td width=\"25%\"><select name=\"usergroup\" class=\"general\">$group</select></td><td width=\"20%\">&nbsp;</td></tr>\r\n\r\n<tr bgcolor=\"#ffffff\" width=\"1000\">\r\n\r\n<td width=\"15%\" valign=\"top\">\r\nPermissions <sup>[<a class=\"info\" onClick=\"return false;\">?<span>These checkboxes set the content access permissions.</span></a>]</sup> :</td>\r\n\r\n<td width=\"25%\">Group: $gpermissions<br>Public&nbsp;: $wpermissions</td>\r\n<td width=\"15%\" valign=\"top\">Content-type <sup>[<a class=\"info\" onClick=\"return false;\">?<span>This attribute defines the content media type.</span></a>]</sup> : </td><td width=\"25%\" valign=\"top\"><input type=\"text\" class=\"input\" name=\"type\" value=\"$type\" size=\"25\">&nbsp;<a href=\"#\" onClick=\"document.editcontent.type.value=\'$mimetype\'; return false;\">Find</a></td>\r\n<td width=\"20%\">&nbsp;</td>\r\n</tr>\r\n\r\n<tr bgcolor=\"#ffffff\" width=\"1000\"><td colspan=\"5\" width=\"100%\">\r\nComment <sup>[<a class=\"info\" onClick=\"return false;\">?<span>Define comments about this content here.\r\n</span></a>]</sup> :<br>\r\n\r\n<textarea cols=\"150\" rows=\"8\" name=\"icomment\" class=\"input\">$icomment</textarea>\r\n</td></tr>\r\n</table>\r\n\r\n</td></tr>\r\n\r\n<tr width=\"1000\"><td colspan=\"3\">\r\nContents <sup>[<a class=\"info\" onClick=\"return false;\">?<span>This is the actual Content body.</span></a>]</sup> : \r\n</td><td><a href=\"#\" onClick=\"a=document.getElementById(\'tags\'); if(a.style.display==\'inline\'){a.style.display=\'none\';}else {a.style.display=\'inline\';} return false;\">Tags</a> <a href=\"#\" onclick=\"\r\nvar o=document.getElementById(\'attributes\'); if(o.style.display==\'inline\'){o.style.display=\'none\';}else{o.style.display=\'inline\';} return false;\">Attributes</a>\r\n</td></tr>\r\n<tr width=\"1000\"><td colspan=\"4\">\r\n<textarea cols=\"150\" rows=\"25\" name=\"body\" class=\"input\"$disabled>$body</textarea></td>\r\n<td>&nbsp;</td>\r\n</table>','0','I\'m not sure what this is...','','1','','2007-08-28 13:45:36','.admin-content','admin','0000','.admin','Content Administration','','admin');
