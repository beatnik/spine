insert into content (body,breaks,icomment,keywords,logging,macros,modified,name,owner,permissions,style,title,type,usergroup) values ('<table width=\"1000\" border=\"0\" bgcolor=\"#ffffff\" cellpadding=\"0\" cellspacing=\"0\">\r\n<tr width=\"1000\">\r\n<form method=\"post\" action=\"<?SPINE_Location?>admin/navbar/save/\">\r\n\r\n<td colspan=\"5\">$error<b>Navigation Bar Edit : $filename</b><br>\r\n<input type=\"submit\" value=\"Save\" class=\"button\">\r\n<input type=\"button\" value=\"Advanced\" onclick=\"o=document.getElementById(\'advanced\'); if(o.style.display==\'inline\'){o.style.display=\'none\';}else{o.style.display=\'inline\';}\" class=\"button\">\r\n<br><b>Settings :</b></td></tr>\r\n<tr bgcolor=\"#ffffff\" >\r\n<td>\r\n<table width=\"100%\" border=\"0\" bgcolor=\"#ffffff\" cellpadding=\"0\" cellspacing=\"0\" id=\"advanced\" style=\"display:none\">\r\n<tr bgcolor=\"#ffffff\">\r\n<td width=\"20%\">Owner<sup>[<a class=\"info\" onClick=\"return false;\">?<span>This shows the navbar owner.</span></a>]</sup><br>$owner</td>\r\n<td width=\"20%\">Usergroup<sup>[<a class=\"info\" onClick=\"return false;\">?<span>This shows the navbar ownergroup.</span></a>]</sup><br><select name=\"usergroup\" class=\"general\">$usergroup</select></td>\r\n<td width=\"20%\">Group Permissions<sup>[<a class=\"info\" onClick=\"return false;\">?<span>These checkboxes set the navbar group access permissions.</span></a>]</sup><br>$gpermissions</td>\r\n<td width=\"20%\">Public Permissions<sup>[<a class=\"info\" onClick=\"return false;\">?<span>These checkboxes set the navbar public access permissions.</span></a>]</sup><br>$wpermissions</td>\r\n<td width=\"20%\">Modified<sup>[<a onClick=\"return false;\" class=\"info\">?<span>This is the last modifcation date of the navbar.</span></a>]</sup><br>$modified</td>\r\n</td></tr></table>\r\n</tr>\r\n<input type=\"hidden\" name=\"type\" value=\"$type\">\r\n<input type=\"hidden\" name=\"name\" value=\"$filename\">\r\n\r\n<tr width=\"80%\"  bgcolor=\"#ffffff\">\r\n<td colspan=\"5\">\r\n<input type=\"hidden\" name=\"name\" value=\"$filename\">\r\n<input type=\"hidden\" name=\"action\" value=\"settings\">\r\n<table border=\"0\" cellspacing=0 cellpadding=0><tr bgcolor=\"#ffffff\">\r\n<td>Alignment <sup>[<a onClick=\"return false;\" class=\"info\">?<span>This attribute defines the horizontal alignment.</span></a>]</sup>\r\n</b></td>\r\n<td>Positioning <sup>[<a onClick=\"return false;\" class=\"info\">?<span>This attribute defines the navigation bar positioning.</span></a>]</sup></td>\r\n<td>Font <sup>[<a onClick=\"return false;\" class=\"info\">?<span>This attribute defines the font face.</span></a>]</sup></td>\r\n<td>Size <sup>[<a onClick=\"return false;\" class=\"info\">?<span>This attribute defines the font size.</span></a>]</sup></td>\r\n<td>Color <sup>[<a onClick=\"return false;\" class=\"info\">?<span>this attribute defines the font color.</span></a>]</sup></td>\r\n<td>Style <sup>[<a onClick=\"return false;\" class=\"info\">?<span>This attribute defines the font style.</span></a>]</sup></td>\r\n<td>Separator  <sup>[<a onClick=\"return false;\" class=\"info\">?<span>This attribute defines the separator used to join each link in the navigation bar.</span></a>]</sup></td>\r\n\r\n</tr>\r\n\r\n<tr><td><select name=\"alignment\">$alignment</select>&nbsp;</td>\r\n<td><select name=\"positioning\">$positioning</select>&nbsp;</td>\r\n<td><input type=\"text\" size=\"9\" name=\"font\" value=\"$font\">&nbsp;</td>\r\n<td><input type=\"text\" size=\"3\" name=\"size\" class=\"input\" value=\"$size\">&nbsp;</td>\r\n<td><input type=\"text\" size=\"7\" name=\"color\" class=\"input\" value=\"$color\">&nbsp;</td>\r\n\r\n<td><select name=\"style\">$style</select>&nbsp;</td>\r\n<td><input type=\"text\" size=\"7\" name=\"separator\" class=\"input\" value=\"$separator\">&nbsp;</td>\r\n</form></tr></table>\r\n\r\n<hr width=\"95%\">\r\n\r\n<!-- Button List -->\r\n\r\n<table width=\"60%\" border=\"0\" bgcolor=\"#ffffff\" cellpadding=\"0\" cellspacing=\"0\">\r\n<tr>\r\n<td>URL <sup>[<a onClick=\"return false;\" class=\"info\">?<span>Define the content name or URL here.</span></a>]</sup></td>\r\n\r\n<td>Image <sup>[<a onClick=\"return false;\" class=\"info\">?<span>Define an optional image URL to create a clickable image.</span></a>]</sup></td>\r\n\r\n<td>Label <sup>[<a onClick=\"return false;\" class=\"info\">?<span>Define the link text here.</span></a>]</sup></td>\r\n\r\n\r\n<td>&nbsp;</td>\r\n<td>&nbsp;</td>\r\n<td>&nbsp;</td>\r\n<td>&nbsp;</td>\r\n</tr>\r\n\r\n<tr>\r\n<form method=\"post\" action=\"<?SPINE_Location?>admin/navbar/addbutton/\">\r\n<input type=\"hidden\" name=\"name\" value=\"$filename\">\r\n<input type=\"hidden\" name=\"id\" value=\"$navbar\">\r\n<input type=\"hidden\" name=\"position\" value=\"$position\">\r\n<td align=\"center\"><input type=\"text\" size=\"25\" name=\"link\" class=\"input\" value=\"\"></td>\r\n<td align=\"center\"><input type=\"text\" size=\"25\" name=\"image\" class=\"input\" value=\"\"></td>\r\n<td align=\"center\"><input type=\"text\" size=\"25\" name=\"label\" class=\"input\" value=\"\"></td>\r\n<td align=\"center\"><input type=\"image\" src=\"<?SPINE_Images?>save.png\" alt=\"Save\"></td>\r\n<td>&nbsp;</td>\r\n<td>&nbsp;</td>\r\n<td>&nbsp;</td>\r\n</form>\r\n</tr>\r\n\r\n$buttonlist\r\n\r\n</table>\r\n\r\n</td></table></font><hr width=\"95%\">Preview:<br>\r\n<?SPINE_Navbar(\"$filename\")?>','0','','','1','','2007-02-22 23:07:23','.admin-navbar','admin','0000','.admin','Navigation Bar Administration','','admin');
